    "react": "^18.2.0"
  },
  "resolutions": {
    "@types/react": "^16.8.23",
    "browserslist": "^4.16.5",
    "css-what": "^5.0.1",
    "glob-parent": "^5.1.2",
    "trim": "^0.0.3"
  }
