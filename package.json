    "babel-eslint": "^8.2.3",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.0.0",
    "babel-preset-react-app": "^9.0.0",
    "babel-register": "^6.26.0",
    "browser-sync": "^2.27.5",
